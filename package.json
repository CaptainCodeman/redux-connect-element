{
  "name": "@captaincodeman/redux-connect-element",
  "version": "1.0.0",
  "description": "Redux HTMLElement Connector",
  "main": "connect.cjs.js",
  "browser": "connect.min.js",
  "esnext": "connect.js",
  "types": "connect.d.ts",
